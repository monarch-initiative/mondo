is_a: HP:0011830 ! Abnormal oral mucosa morphology
property_value: http://purl.org/dc/elements/1.1/date 2018-12-02T14:06:55Z xsd:dateTime
created_by: peter

[Term]
id: HP:0032158
name: Unusual infection by anatomical site
namespace: human_phenotype
def: "An unusual infection classified by the affected body part." []
is_a: HP:0032101 ! Unusual infection
property_value: http://purl.org/dc/elements/1.1/date 2018-12-09T12:17:59Z xsd:dateTime
created_by: peter

[Term]
