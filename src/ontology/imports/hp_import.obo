created_by: peter

[Term]
id: HP:0031983
name: Abnormal pulmonary thoracic imaging finding
namespace: human_phenotype
def: "This term groups terms representing abnormal findings derived from chest X-ray investigation of the lung. In general, lung abnormalities can manifest as opacities (areas of increased density) or as regions with decreased density." []
synonym: "Abnormal chest radiograph finding (lung)" EXACT []
is_a: HP:0012252 ! Abnormal respiratory system morphology
property_value: http://purl.org/dc/elements/1.1/date 2018-07-08T13:38:01Z xsd:dateTime
created_by: peter

[Term]
id: HP:0032016
name: Abnormal sputum
namespace: human_phenotype
def: "Abnormal appearance of material expectorated (coughed up) from the respiratory system and that is composed of mucus but may contain other substances such as pus, blood, microorganisms, and fibrin." []
synonym: "Abnormal sputum morphology" EXACT []
is_a: HP:0012252 ! Abnormal respiratory system morphology
property_value: http://purl.org/dc/elements/1.1/date 2018-08-25T12:36:36Z xsd:dateTime
created_by: peter

[Term]
id: HP:0032039
name: Abnormality of the ocular adnexa
namespace: human_phenotype
